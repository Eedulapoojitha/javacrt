import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        
         Scanner s=new Scanner(System.in);
        int max=0;
        int n=s.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++){
            a[i]=s.nextInt();
            if(a[i]>max){
                max=a[i];}
        }
        System.out.println(max);
        
    }
}
    
